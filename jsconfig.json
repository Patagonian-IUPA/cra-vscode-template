{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "checkJs": false,
    "jsx": "react",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "skipDefaultLibCheck": true,
    "moduleResolution": "node"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
